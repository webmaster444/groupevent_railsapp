<%= form_with scope: :groupevent, url: groupevents_path, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </p>
  
  <p>
    <%= form.label :startdate %><br>    
    <%= form.text_field :startdate, "data-provide"=>'datepicker' %>
  </p>
  <p>
    <%= form.label :duration %><br>
    <%= form.text_field :duration %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>